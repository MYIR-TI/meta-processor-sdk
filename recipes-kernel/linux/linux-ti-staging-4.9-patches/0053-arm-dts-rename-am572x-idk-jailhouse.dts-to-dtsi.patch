+};
+
+/ {
+	ocp {
+		pruss1_eth {
+			status = "disabled";
+		};
+
+		pruss2_eth {
+			status = "disabled";
+		};
+
+		crossbar_mpu: crossbar@4a002a48 {
+			ti,irqs-skip = <10 44 127 129 133 134 135 136 137 139 140>;
+		};
+	};
+};
