From bf2d7adad68dd2e929469d648cf781660d217cb8 Mon Sep 17 00:00:00 2001
From: Vitaly Andrianov <vitalya@ti.com>
Date: Fri, 22 Jul 2016 22:58:58 +0000
Subject: [PATCH 34/34] ARM: dts: k2e: add dts node for k2e hw_rng driver

This patch adds dts node for k2e hw_random generator driver

Signed-off-by: Vitaly Andrianov <vitalya@ti.com>
Signed-off-by: Jacob Stiffler <j-stiffler@ti.com>
---
 arch/arm/boot/dts/keystone-k2e-netcp.dtsi | 20 ++++++++++++++++++++
 1 file changed, 20 insertions(+)

diff --git a/arch/arm/boot/dts/keystone-k2e-netcp.dtsi b/arch/arm/boot/dts/keystone-k2e-netcp.dtsi
index 4051720..812497e 100644
--- a/arch/arm/boot/dts/keystone-k2e-netcp.dtsi
+++ b/arch/arm/boot/dts/keystone-k2e-netcp.dtsi
@@ -668,3 +668,23 @@ netcpx: netcpx@2f00000 {
 		};
 	};
 };
+
+sa_subsys: subsys@24080000 {
+	#address-cells = <1>;
+	#size-cells = <1>;
+	ranges = <0 24080000 0x40000>;
+	compatible = "simple-bus";
+
+	sa_config: subsys {
+		compatible      = "syscon";
+		reg             = <0x0 0x100>;
+	};
+
+	hwrng {
+		compatible = "ti,keystone-rng";
+		sa-cfg = <&sa_config>;
+		clocks = <&clksa>;
+		clock-names = "fck";
+		reg = <0x24000 0x1000>;
+	};
+};
-- 
